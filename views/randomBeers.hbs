<div>
  <h1>Random Beer</h1>
  {{#each this.randomBeer}}
    <img src='{{this.image_url}}' alt='' />
    <p><strong>{{this.name}}</strong></p>
    <p>{{this.tagline}}</p>
    <p>{{this.description}}</p>
    <p>{{this.food_pairing}}</p>
    <p>{{this.brewers_tips}}</p>
  {{/each}}
</div>

{{> footerPartial}}
